{
  "name": "spk/si-keanggotaan",
  "description": "Sistem Informasi Registrasi dan Keanggotaan Serikat Pekerja Kampus",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "codeigniter4/framework": "^4.5",
    "codeigniter4/settings": "*",
    "codeigniter4/shield": "^1.0",
    "dompdf/dompdf": "^3.0",
    "endroid/qr-code": "^5.0",
    "league/csv": "^9.0",
    "phpoffice/phpspreadsheet": "^2.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "kint-php/kint": "*",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^10.5",
    "predis/predis": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Config\\": "app/Config/"
    },
    "exclude-from-classmap": [
      "**/Database/Migrations/**"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Support\\": "tests/_support"
    }
  },
  "scripts": {
    "test": "phpunit",
    "post-update-cmd": [
      "CodeIgniter\\ComposerScripts::postUpdate"
    ]
  },
  "support": {
    "source": "https://github.com/spk/si-keanggotaan",
    "issues": "https://github.com/spk/si-keanggotaan/issues"
  },
  "prefer-stable": true,
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
